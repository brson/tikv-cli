command = {
  SOI ~
  (put | get)
  ~ EOI
}

put = { "put" ~ key ~ value }
get = { "get" ~ key }

key = { string }
value = { string }

string = _{ quoted_string | unquoted_string }
quoted_string = @{ QUOTATION_MARK ~ ASCII_ALPHANUMERIC* ~ QUOTATION_MARK }
unquoted_string = @{ (LETTER | NUMBER)+ }

WHITESPACE = _{ " " }
